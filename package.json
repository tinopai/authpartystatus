    "humanize-duration": "^3.27.2",
    "memory-cache": "^0.2.0",
    "ping": "^0.4.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "twemoji": "^14.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "@types/humanize-duration": "^3.27.1",
    "@types/memory-cache": "^0.2.2",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "autoprefixer": "^10.4.7",
